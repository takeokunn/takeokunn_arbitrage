{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "current"
            },
            "modules": false,
            "loose": true
        }]
    ],
    "plugins": [
        [
            "module-resolver", {
                "root": ["./src"],
                "alias": {
                    "actions": "./src/actions",
                    "config": "./src/config",
                    "constants": "./src/constants",
                    "reducers": "./src/reducers",
                    "sagas": "./src/sagas",
                    "services": "./src/services",
                }
            },
            "transform-function-bind",
            "transform-export-extensions",
            "external-helpers"
        ]
    ]
}
